<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="testsuite" verbose="1" >
    <listeners>
        <listener class-name="com.nopcommerce.demo.customlisteners.CustomListeners"/>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
    </listeners>
    <test name="CopmuterTest" >
        <classes>
            <class name="com.nopcommerce.demo.testsuit.CopmuterTest"/>
        </classes>
    </test>
    <test name="LoginTest">
        <classes>
            <class name="com.nopcommerce.demo.testsuit.LoginTest"></class>
        </classes>
    </test>
    <test name="RegisterTest">
        <classes>
            <class name="com.nopcommerce.demo.testsuit.RegisterTest"></class>
        </classes>
    </test>
</suite>


